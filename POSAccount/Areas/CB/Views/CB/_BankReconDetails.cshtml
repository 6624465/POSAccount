@model List<POSAccount.Contract.BankRecon>

@for(var i = 0; i < Model.Count; i++)
{
    <tr>
        <td>0</td>
        <td>@Model[i].LocalAmount.ToString("#####0.00")</td>
        <td>@Model[i].DocumentDate.ToString("dd/MM/yyyy")</td>
        <td>@Model[i].DocumentType</td>
        <td>@Model[i].DocumentNo</td>
        <td>@Model[i].ChequeNo</td>
        <td>@Model[i].Remark</td>
        <td>@Model[i].ReceiptAmount.ToString("#####0.00")</td>
        <td>@Model[i].PaymentAmount.ToString("#####0.00")</td>
        <td>@Model[i].DateReconciled.ToString("dd/MM/yyyy")</td>
    </tr>
}


