@using POSAccount.ViewModals.Search


@model POSAccount.ViewModals.Search.SearchResultVm<POSAccount.ViewModals.Search.APInvoice>

<div class="modal-body">
    @{ Html.RenderPartial("_SearchBoxControls", Model.searchWrapper); }   
    <div class="row">
        <div class="col-lg-12 col-xs-12">
            <div class="md-table">
                <table class="table table-hover table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>Agent Code</th>
                            <th>Agent Name</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (var i = 0; i < Model.ListData.Count; i++)
                        {
                            <tr class="cursorCss" onclick="trClick('tr_@i','@Model.searchWrapper.searchVm.SearchTable')" 
                                id="tr_@i" 
                                data-agent-code="@Model.ListData[i].AgentCode" 
                                data-agent-name="@Model.ListData[i].AgentName">
                                <td>@Model.ListData[i].AgentCode</td>
                                <td>@Model.ListData[i].AgentName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

